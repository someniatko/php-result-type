    "description": "Functional-ish generic Result type: either Success or Error",
    "require": {
        "php": "8.2.* || 8.3.* || 8.4.* || 8.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5 || ^12.5",
        "vimeo/psalm": "^6",
        "psalm/plugin-phpunit": "^0.19",
        "squizlabs/php_codesniffer": "^4"
    },
    "autoload": {
        "psr-4": {
            "Someniatko\\ResultType\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    }
}
